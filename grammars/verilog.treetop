      rule timeskew_timing_check
        "$timeskew" s "(" s reference_event s "," s data_event s "," s timing_check_limit s
        (s "," s notifier? (s "," s event_based_flag? (s "," s remain_active_flag?)? )? )? s ")" s ";" {
          def to_ast
