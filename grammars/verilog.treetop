      # $skew_timing_check ::=
      #   $skew ( reference_event , data_event , timing_check_limit [ , [ notifier ] ] ) ;
      rule skew_timing_check
        "$skew" s "(" s reference_event s "," s data_event s "," s timing_check_limit (s "," s notifier?)? s ")" s ";" {
          def to_ast
            n :skew_timing_check, *elements_to_ast
