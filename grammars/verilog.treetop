        }
      end
      # $period_timing_check ::=
      #   $period ( controlled_reference_event , timing_check_limit [ , [ notifier ] ] ) ;
      rule period_timing_check
        "$period" s "(" s controlled_reference_event s "," s timing_check_limit (s "," s notifier?)? s ")" s ";" {
          def to_ast
