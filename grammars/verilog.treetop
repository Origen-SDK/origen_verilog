      # loop_generate_construct ::=
      #   for ( genvar_initialization ; genvar_expression ; genvar_iteration )
      #     generate_block
      rule loop_generate_construct
        "for" s "(" s genvar_initialization s ";" s genvar_expression s ";" s genvar_iteration s ")" s
        generate_block {
